   "source": [
    "def P(n, memo):\n",
    "    if n in memo:\n",
    "        return memo[n]\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        memo[n] = 1 - (0.5 * P(n-1, memo))\n",
    "        return memo[n]\n",
    "\n",
    "memo = {}\n",
    "\n",
    "probabilities = {n: P(n, memo) for n in range(1, 26)}\n",
    "probabilities"
   ]
